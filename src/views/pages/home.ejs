<% include ../partials/header.ejs %>

<div class="flex-container super-container">
  <div class="menu flex-container vertical center">
    <div class="menu-option menu-hover" data-menu="pax">Pax Pods</div>
    <div class="menu-option menu-hover" data-menu="normal">All Items</div>
    <div class="menu-option menu-hover selected" data-menu="sales-by-product">Sales By Product</div>
    <div class="hidden pax page">
      <div class="change-view-type">
        <div class="view-type-option menu-hover" data-option="price-delivered">By Price Delivered</div>
        <div class="view-type-option menu-hover selected" data-option="price-undelivered">By Price Undelivered</div>
        <div class="view-type-option menu-hover" data-option="price-all">By Price All</div>
        <div class="view-type-option menu-hover" data-option="customer">By Customer</div>
        <div class="view-type-option menu-hover hidden" data-option="product">By Product</div>
      </div>
      <div class="toggles">
        <div class="toggle-option menu-hover-alt" data-target="due-date">Show Sales Order #'s</div>
        <div id="date-restricted" class="toggle-option menu-hover-alt selected-alt" data-trigger="pax-pods">Show Last Month Only</div>
      </div>
    </div>
  </div>
  <div class="main-body flex-container">
    <div class="hidden table pax page">
      <div id="pax-pods" class="flex-container">
        <div id="by-price-delivered" class="flex-container vertical hidden"></div>
        <div id="by-price-undelivered" class="flex-container vertical"></div>
        <div id="by-price-all" class="flex-container vertical hidden"></div>
        <div id="by-customer" class="flex-container vertical hidden"></div>
        <div id="by-product" class="flex-container vertical hidden"></div>
      </div>
    </div>
    <div class="hidden table normal all-items-list page">
      <div class="by-customer flex-container vertical hidden"></div>
      <div id="brand-list" class="flex-container"></div>
    </div>
    <div class="table page sales-by-product">
      <div class="flex-container list"></div>
    </div>
  </div>
</div>

<% include ../partials/footer.ejs %>